@model List<NamekWebPes.Models.MemberModel>
<table class="table table-hover border">
    <thead>
        <tr>
            <th scope="col">STT</th>
            <th scope="col">Tên người chơi</th>
     


        </tr>
    </thead>
    <tbody>
        @{
            int stt = 0;
            foreach (var item in Model)
            {
                stt++;
                <tr>
                    <th scope="row">@stt</th>
                    <td>@item.Membername</td>
                </tr>

            }
        }
    </tbody>
</table>